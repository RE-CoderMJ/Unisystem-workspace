		<settings>
			<setting name="jdbcTypeForNull" value="NULL" />
		</settings>
		
		<typeAliases>
			<typeAlias type="com.us.uni.users.model.vo.Users" alias="Users" />
			<typeAlias type="com.us.uni.board.model.vo.Board" alias="Board" />
			<!-- 
			<typeAlias type="com.us.uni.student.model.vo.Student" alias="Student" />
			 <typeAlias type="com.us/uni/Professor.model.vo.Professor" alias="Professor" />
			 -->
